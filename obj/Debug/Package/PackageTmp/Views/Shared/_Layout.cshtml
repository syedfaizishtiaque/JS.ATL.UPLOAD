<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ATL FILES</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Content/overlayScrollbars/css/OverlayScrollbars.min.css">
    <link href="~/Content/DataTables/css/jquery.dataTables.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/buttons.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/daterangepicker/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/responsive.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/buttons.bootstrap4.min.css" rel="stylesheet" />
    @*<link href="~/Content/plugins/chart.js/Chart.min.css" rel="stylesheet" />*@
    <style>
        .nav-link {
            color: white !important;
        }

        .btn-primary {
            background: darkcyan !important;
            border-color: darkcyan !important;
        }
    </style>
</head>
<body class="sidebar-collapse">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!-- Site wrapper -->
        <!-- Preloader -->
        <div id="preloaderjq" class="preloader flex-column justify-content-center align-items-center" style="background:none!important">
            <img id="imgloader" src="~/Content/preloader/Blocks-1.3s-237px.gif" alt="CCLogo" height="200" width="200" />

        </div>
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button" style="color:grey!important"><i class="fas fa-bars"></i></a>
                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Navbar Search -->
                <li class="nav-item">
                    @{

                        if (ATL_UPLOAD.Models.AppSession.Session != null)
                        {
                            <a href="#" class="nav-link" style="color:grey!important"><b><span> <span class="hidden-xs">@(ATL_UPLOAD.Models.AppSession.Session.username.ToUpper())</span></span></b></a>
                        }
                        else
                        {
                            <a href="#" class="nav-link" style="color:grey!important"><b><span> <span class="hidden-xs">Anonymous User</span></span></b></a>
                        }
                    }

                </li>

                <!-- Messages Dropdown Menu -->
                <li class="nav-item dropdown">
                    @*<a class="nav-link" style="color:grey!important" data-toggle="dropdown" href="#" title="Log Out" onclick="Logout()">
                        <i class="fa fa-sign-out-alt"></i>
                        <span class="badge badge-danger navbar-badge"></span>
                    </a>*@

                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <aside class="main-sidebar sidebar-primary elevation-4 bg-cyan">
            <!-- Brand Logo -->
            <a href="#" class="brand-link bg-info">
                <img src="~/Content/dist/img/JSLOGO.jpg" alt="Js Bank" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">JSBL - ATL Excel Upload</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <!-- SidebarSearch Form -->
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul id="sidebarcustom" class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a class="nav-link" href="/Home/Index">
                                <i class="far fa-eye nav-icon"></i><p>
                                   Upload Excel
                                </p>
                            </a>
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper" style="min-height:0px;">
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0">@ViewBag.Title</h1>
                        </div><!-- /.col -->
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->
            <div class="content">
                @RenderBody()
                <!-- /.container-fluid -->
            </div>
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - JS Bank Ltd - Ancillary Apps.</p>
            </footer>
        </div>
    </div>
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="~/Content/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Content/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/dist/js/adminlte.js"></script>

    @*<script src="~/Content/plugins/moment/datetime.js"></script>*@
    <script src="~/Content/plugins/chart.js/Chart.min.js"></script>
    <script src="~/Content/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="~/Content/plugins/moment/moment.min.js"></script>
    <script src="~/Content/plugins/inputmask/jquery.inputmask.min.js"></script>
    <script src="~/Content/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>

    <script src="~/Content/DataTables/jquery.dataTables.min.js"></script>


    <script src="~/Scripts/DataTables/dataTables.bootstrap4.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.responsive.min.js"></script>
    <script src="~/Scripts/DataTables/responsive.bootstrap4.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.bootstrap4.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.html5.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.print.min.js"></script>
    <script src="~/Scripts/DataTables/buttons.colVis.js"></script>
    <script src="~/Scripts/DataTables/jszip.min.js"></script>
    <script src="~/Scripts/DataTables/pdfmake.min.js"></script>
    <script src="~/Scripts/DataTables/vfs_fonts.js"></script>

    <script type="text/javascript">

        $('.expdate').datetimepicker({
            format: 'L',
        });
        $('.Inqdate').datetimepicker({
            format: 'yyyy-MM-DD',
        });
        $('.ftdate').datetimepicker({
            format: 'L',
        });
        $('.isdate').datetimepicker({

            format: 'L',
            maxDate: 'now',
            // defaultDate: moment(),
            sideBySide: true
        });


      
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
